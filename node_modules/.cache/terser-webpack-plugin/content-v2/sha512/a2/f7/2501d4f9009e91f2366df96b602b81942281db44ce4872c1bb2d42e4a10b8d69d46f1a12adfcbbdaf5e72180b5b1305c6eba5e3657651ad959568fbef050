d';\nimport { Card as CardType } from '../../../types/card';\nimport styles from './Card.module.css';\n\ntype CardProps = {\n  card: CardType;\n  className?: string;\n};\n\nconst Card: FunctionComponent<CardProps> = ({ card, className }) => {\n  const Image = getSuitImage(card);\n\n  return (\n    <div className={`${styles.card} ${className || ''}`}>\n      <div className={styles['card-desc']}>\n        <span\n          className={`${styles['card-desc__rank']} ${\n            isRedCard(card) ? styles['card-desc__rank--red'] : ''\n          }`}\n        >\n          {card.rank}\n        </span>\n        <Image className={styles['card-desc__img']} />\n      </div>\n      <div className={styles['card-suit']}>\n        <Image className={styles['card-suit__img']} />\n      </div>\n    </div>\n  );\n};\n\nexport default Card;\n","import React, { FunctionComponent } from 'react';\nimport styles from './Counter.module.css';\n\ntype CardGameProps = {\n  cardsLeft: number;\n  acesLeft: number;\n};\n\nconst Counter: FunctionComponent<CardGameProps> = ({ cardsLeft, acesLeft }) => {\n  return (\n    <div className={styles.counter}>\n      <span className={`${styles.counter__number} font-secondary`}>{cardsLeft}</span>\n      <span className={`${styles.counter__text} font-secondary`}>Cards Left</span>\n      <div className={`${styles['counter-aces']} font-secondary`}>\n        {[...Array(acesLeft)].map((_, i) => (\n          <span className={styles['counter-aces__ace']} key={i}>\n            A\n          </span>\n        ))}\n      </div>\n    </div>\n  );\n};\n\nexport default Counter;\n","import React, { FunctionComponent } from 'react';\nimport Confetti from 'react-confetti';\nimport { ReactComponent as Winner } from '../../assets/img/winner.svg';\nimport useCardDeck from '../../hooks/useCardDeck';\nimport Card from './Card/Card';\nimport Counter from './Counter/Counter';\nimport styles from './CardGame.module.css';\n\nconst CardGame: FunctionComponent = () => {\n  const { deal, makeDeal, reset, cardsLeft, acesLeft } = useCardDeck();\n\n  const win = cardsLeft === 0 && acesLeft === 0;\n  const loss = cardsLeft !== 0 && acesLeft === 0;\n  const hasEndedGame = loss || win;\n\n  return (\n    <div className={styles.game}>\n      {win ? <Confetti gravity={0.5} numberOfPieces={100} recycle={false} /> : null}\n      {win ? (\n        <div className={styles['game-win']}>\n          <Winner />\n        </div>\n      ) : null}\n      <div className={styles['game-counter']}>\n        <Counter cardsLeft={cardsLeft} acesLeft={acesLeft} />\n      </div>\n      <div className={styles['game-cards']}>\n        <div className={styles['game-cards__container']}>\n          {deal.map((card, index) => (\n            <Card\n              className={`${styles['game-cards__card']} ${\n                deal.length === 5 ? styles[`game-cards__card-${index}`] : ''\n              }`}\n              key={`${card.rank} ${card.suit}`}\n              card={card}\n            />\n          ))}\n        </div>\n      </div>\n\n      <div className={`${styles['game-deal']}`}>\n        {loss ? (\n          <span className={`${styles['game-lose']} font-secondary`}>\n            You lose.\n            <br /> Better luck next time!\n          </span>\n        ) : null}\n        {hasEndedGame ? (\n          <button\n            className={`${styles['game-reset-btn']} btn-secondary`}\n            type=\"button\"\n            onClick={reset}\n          >\n            Play Again\n          </button>\n        ) : (\n          <button\n            className={`${styles['game-deal__button']} btn-primary`}\n            type=\"button\"\n            onClick={makeDeal}\n          >\n            Deal\n          </button>\n        )}\n      </div>\n      {hasEndedGame ? null : (\n        <div className={styles['game-reset']}>\n          <button\n            className={`${styles['game-reset-btn']} ${styles['game-reset__button']} btn-secondary`}\n            type=\"button\"\n            onClic